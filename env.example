# Telegram Bot Configuration
BOT_TOKEN=your_bot_token_here
ADMIN_IDS=123456789,987654321

# X-UI Panel Configuration
XUI_URL=http://your-xui-panel.com:port/path
XUI_USER=admin
XUI_PASSWORD=your_password

# YooKassa Payment Configuration
USE_YOOKASSA=true
YOOKASSA_ACCOUNT_ID=your_shop_id
YOOKASSA_SECRET_KEY=test_your_test_key_or_live_your_live_key
YOOKASSA_RETURN_URL=https://t.me/your_bot_username

# Server Configuration (опционально, если нужно переопределить значения из config.py)
SERVER_HOST=your_server_ip
SERVER_PORT=443
REALITY_PBK=your_reality_public_key
REALITY_SID=your_reality_short_id
REALITY_SNI=your_reality_sni
REALITY_FP=chrome

# Tariff configuration
TARIFF_STANDARD_INBOUND_ID=1
TARIFF_STANDARD_SERVER_HOST=your_standard_server_ip
TARIFF_STANDARD_VM_INBOUND_ID=6
TARIFF_STANDARD_VM_SERVER_HOST=your_standard_vm_server_ip
TARIFF_OBHOD_INBOUND_ID=2
TARIFF_OBHOD_SERVER_HOST=your_obhod_server_ip
TARIFF_OBHOD_EMAIL_SUFFIX=-obhod

# Performance Settings (опционально)
CLIENT_INFO_TTL_SEC=300
RATE_MAX_ACTIONS=3
RATE_WINDOW_SEC=1
XUI_LOGIN_RETRIES=3
XUI_LOGIN_TIMEOUT=10
XUI_LOGIN_COOLDOWN_SEC=60
XUI_CLIENT_IP_LIMIT=6
